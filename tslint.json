{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false
      }
    ],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-console": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
